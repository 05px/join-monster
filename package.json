{
  "name": "join-monster",
  "version": "0.2.6",
  "description": "A GraphQL-to-SQL query execution layer.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "babel src --no-comments --out-dir dist",
    "watch": "babel src --watch --out-dir dist",
    "server": "babel-watch ./example/server.js",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "db:build": "rm -f example/data/data.sl3; echo '.quit' | sqlite3 example/data/data.sl3 -init example/data/setup.sql",
    "db:shell": "sqlite3 --column --header example/data/data.sl3",
    "test": "npm run db:build && ava"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 10,
    "verbose": true,
    "failFast": true,
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stems/join-monster.git"
  },
  "keywords": [
    "graphql",
    "sql",
    "query",
    "schema"
  ],
  "author": "",
  "contributors": [
    "Andrew Carlson <andrew@stem.is>",
    "Matthew Elder <matt@stem.is>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stems/join-monster/issues"
  },
  "homepage": "https://github.com/stems/join-monster#readme",
  "peerDependencies": {
    "graphql": "0.x"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.3-rc0",
    "eslint": "^3.5.0",
    "graphql": "^0.7.0",
    "graphql-relay": "^0.4.3",
    "graphsiql": "0.0.1",
    "knex": "^0.12.0",
    "koa": "^1.2.4",
    "koa-custom-graphiql": "0.0.2",
    "koa-graphql": "^0.5.6",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "sinon": "^1.17.6",
    "sqlite3": "^3.1.4"
  },
  "dependencies": {
    "nesthydrationjs": "^0.3.0"
  }
}
